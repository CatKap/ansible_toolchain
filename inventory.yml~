  hosts:
    tvbox:
      ansible_password: tvbox
  vars:
    ansible_ssh_extra_args: -o PubkeyAuthentication=no -o HostKeyAlgorithms=+ssh-rsa
      -o PasswordAuthentication=yes
    ansible_user: root

